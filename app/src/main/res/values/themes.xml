<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.FrutosSecosApp" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.FrutosSecosApp" parent="Base.Theme.FrutosSecosApp" />

    <style name="Theme.FrutosSecosApp.NoActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
    </style>

    <style name="Theme.FrutosSecosApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FrutosSecosApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Base.Theme.UnitConverter" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>
    <style name="Theme.AppCompat.Light.Custom" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">#0F7EC8</item> <!-- Action Bar background color -->
        <item name="android:textColorPrimary">#FFFFFF</item> <!-- Title text color -->
        <item name="colorPrimaryDark">#000000</item> <!-- Status bar color -->
        <item name="android:windowLightStatusBar">true</item> <!-- Light status bar text -->
    </style>
    <style name="Theme.UnitConverter" parent="Base.Theme.UnitConverter" />



</resources>